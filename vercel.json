{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "framework": "vite",
  "functions": {
    "app/entry.server.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}